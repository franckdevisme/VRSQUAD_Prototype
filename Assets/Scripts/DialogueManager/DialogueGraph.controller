  m_Script: {fileID: 11500000, guid: ee55f61d436d52c4492c97a91066cf90, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  npc_Index: 0
  audioclip: {fileID: 8300000, guid: 444f4be64d6c19943a8676ee3258107f, type: 3}
  dialogue:
  - textToDisplay: "L\u2019intelligence spatiale est la \xAB capacit\xE9 de trouver
      son chemin dans un environnement"
    displayDelay: 4
  - textToDisplay: "donn\xE9 et d'\xE9tablir des relations entre les objets dans l'espace
      \xBB. "
    displayDelay: 4
  - textToDisplay: "Elle permet de voir la continuit\xE9 d'une image en rotation dans
      l'espace, de cr\xE9er une image mentale."
    displayDelay: 5
  - textToDisplay: 'Par exemple, elle permet de bien arranger des objets dans un espace
      comme des valises dans un coffre de voiture un plan de  '
    displayDelay: 6
  - textToDisplay: "route pour aller d'un point \xE0 un autre. Elle est utilis\xE9e
      dans des domaines comme l'architecture, la menuiserie ou l'urbanisme."
    displayDelay: 6
  - textToDisplay: "Elle est utile en math\xE9matiques et dans le jeu d'\xE9checs."
    displayDelay: 4
  pauseDelay: 1
