  m_Script: {fileID: 11500000, guid: ee55f61d436d52c4492c97a91066cf90, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  npc_Index: 0
  audioclip: {fileID: 8300000, guid: 8fe843078d6a4e5419fbe95a3ee31ac0, type: 3}
  dialogue:
  - textToDisplay: "\xAB Ce qui se con\xE7oit bien, s\u2019\xE9nonce clairement. Et
      les mots pour le dire arrivent ais\xE9ment. \xBB"
    displayDelay: 5
  - textToDisplay: "Pour commencer, il est essentiel de se sentir \xE0 l'aise avec
      soi-m\xEAme"
    displayDelay: 3.5
  - textToDisplay: "pour prendre la parole. En effet, si nous doutons ou h\xE9sitons
      avec notre fa\xE7on de parler,"
    displayDelay: 5
  - textToDisplay: "ou le contenu de notre projet, notre prise de parole s\u2019en
      ressentira, \xE0 travers des manifestations ext\xE9rieures  \r\n"
    displayDelay: 6
  - textToDisplay: "comme par exempe des h\xE9sitations, des b\xE9gaiements, le regard
      fuyant ou la tonalit\xE9 de voix non convaincue...\n"
    displayDelay: 5
  pauseDelay: 2
